{
  "name": "react-evefyou-common",
  "version": "1.0.13",
  "description": "",
  "type": "module",
  "main": "./cjs/index.js",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./es/index.js",
      "types": "./es/index.d.ts"
    },
    "./utils": {
      "require": "./cjs/utils/index.js",
      "import": "./es/utils/index.js",
      "types": "./es/utils/index.d.ts"
    }
  },
  "files": [
    "cjs",
    "es"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsup common/index.ts --dts --format=esm,cjs && vite build",
    "build:tsc": "tsup common/index.ts --dts",
    "clean": "rimraf node_modules",
    "build:space": "tsc && cross-env NODE_OPTIONS=--max-old-space-size=4096 vite build",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:build": "rimraf es && rimraf cjs && rimraf css && rimraf dist && rimraf locale",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EvefyouFE/react-evefyou-common.git"
  },
  "keywords": [
    "react",
    "js",
    "common",
    "hooks",
    "utils",
    "evefyou"
  ],
  "author": "EvefyouFE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EvefyouFE/react-evefyou-common/issues"
  },
  "homepage": "https://github.com/EvefyouFE/react-evefyou-common#readme",
  "peerDependencies": {
    "moment": ">=2.29.4",
    "ramda": ">=0.29.0",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "react-intl": ">=6.4.4",
    "uuid": ">=9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.1",
    "@types/ramda": "^0.29.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "rimraf": "^5.0.1",
    "tslib": "^2.6.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.1",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
